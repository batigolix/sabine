{% set classes = [
  'paragraph',
  'paragraph--type--' ~ paragraph.bundle|clean_class,
  view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
  not paragraph.isPublished() ? 'paragraph--unpublished',
  background
] %}
<section id="contact" {{ attributes.addClass(classes) }}>
  <div class="container">
    <header>
      <h2 class="alt">{{ 'Contact'|t }}</h2>
    </header>
    {{ content.field_body }}
    {{ contact_form }}
  </div>
</section>
